{
  "name": "kittik",
  "version": "0.0.0",
  "description": "Show off your slides in terminal",
  "license": "MIT",
  "homepage": "https://github.com/ghaiklor/kittik#readme",
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://ghaiklor.com"
  },
  "bugs": {
    "url": "https://github.com/ghaiklor/kittik/issues",
    "email": "ghaiklor@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghaiklor/kittik.git"
  },
  "scripts": {
    "all": "npm run clean && npm run build && npm run test && npm run lint",
    "bootstrap": "lerna bootstrap --hoist --strict",
    "build": "lerna run build --stream --parallel",
    "clean": "lerna run clean --stream --parallel",
    "commit": "git cz",
    "lint": "lerna run lint --stream --parallel",
    "postinstall": "npm run bootstrap",
    "publish": "lerna version --conventional-commits --exact --create-release github && lerna publish",
    "test": "lerna run test --stream --parallel",
    "update": "find ./src -name package.json -maxdepth 2 -exec dirname {} \\; | xargs -o -L 1 npm-check --save-exact --update && lernaupdate --dedupe"
  },
  "devDependencies": {
    "git-cz": "4.3.1",
    "lerna": "3.20.2",
    "lerna-update-wizard": "0.17.7",
    "npm-check": "5.9.2"
  }
}
