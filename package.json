{
  "name": "kittik",
  "version": "0.0.0",
  "description": "Show off your slides in terminal",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/ghaiklor/kittik#readme",
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://ghaiklor.com"
  },
  "bugs": {
    "url": "https://github.com/ghaiklor/kittik/issues",
    "email": "ghaiklor@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghaiklor/kittik.git"
  },
  "scripts": {
    "all": "npm run build && npm run test && npm run lint",
    "bootstrap": "lerna bootstrap --hoist --strict",
    "build": "lerna run build",
    "clean": "lerna run clean",
    "commit": "git cz",
    "lint": "lerna run lint",
    "postinstall": "npm run bootstrap",
    "precommit": "npm run lint",
    "prepublish": "npm run all",
    "publish": "lerna publish --dist-tag beta --preid beta --conventional-commits --conventional-prerelease --exact --create-release github",
    "test": "lerna run test",
    "update": "find ./packages -name package.json -maxdepth 2 -exec dirname {} \\; | xargs -o -L 1 npm-check --save-exact --update"
  },
  "devDependencies": {
    "git-cz": "4.3.1",
    "lerna": "3.20.2",
    "npm-check": "5.9.2"
  }
}
