  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghaiklor/kittik.git"
  },
  "scripts": {
    "all": "npm run clean && npm run build && npm run test && npm run lint",
    "bootstrap": "lerna bootstrap --hoist --strict",
    "build": "lerna run build --stream --parallel",
    "clean": "lerna clean && rimraf node_modules",
    "commit": "git cz",
    "lint": "lerna run lint --stream --parallel",
    "publish": "lerna publish",
    "test": "lerna run test --stream --parallel",
    "version": "lerna version --conventional-commits --exact --create-release github"
  },
  "devDependencies": {
    "git-cz": "4.3.1",
    "lerna": "3.20.2",
    "rimraf": "3.0.2"
  }
}
